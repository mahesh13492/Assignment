<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  

	
	<bean id="department" class="com.ByName.Department" autowire="default" >
		<property name="departmentName" value="Development"/>
	</bean>
	<bean id="employee" class="com.ByName.Employee">
      <property name="empName" value="MAhesh" />
      <property name="empAdd" value="Indore" />
  	</bean>
  	<bean id="category" class="com.ByName.Category">
      <property name="catName" value="JAVA DEvELOPER" />
    </bean>

</beans>